{
    "extends": "@quasar/app/tsconfig-preset",
    "compilerOptions": {
        "baseUrl": ".",
        "experimentalDecorators": true,
        "target": "es6",
        "allowSyntheticDefaultImports": true,
        "noImplicitAny": true,
        "noUnusedParameters": true,
        "noUnusedLocals": true,
        "noFallthroughCasesInSwitch": true,
        "esModuleInterop": true,
        "skipLibCheck": true
    }
}
